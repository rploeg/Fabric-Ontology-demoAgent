@prefix : <http://example.com/zava-manufacturing-isa95#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix isa95: <http://www.isa.org/isa-95#> .
@prefix opcua: <http://opcfoundation.org/UA/#> .

# =============================================================================
# Ontology Declaration
# =============================================================================

<http://example.com/zava-manufacturing-isa95> a owl:Ontology ;
    rdfs:label "Zava Inc. - Smart Textile Manufacturing (ISA-95 / OPC UA)" ;
    rdfs:comment "Ontology for smart textile (smart mesh) manufacturing based on ISA-95 equipment hierarchy and OPC UA information model. Models the Make & Quality process from coating development through weaving execution to quality management for ZavaCore smart fiber products." .

# =============================================================================
# Entity Classes  (ISA-95 aligned)
# =============================================================================

:ProductBatch a owl:Class ;
    rdfs:label "ProductBatch" ;
    rdfs:comment "Key: BatchId (string). ISA-95 Material Lot / Product Batch. A production batch of finished smart mesh units (e.g. 10000 units of ZavaCore Field Standard)." .

:ProcessSegment a owl:Class ;
    rdfs:label "ProcessSegment" ;
    rdfs:comment "Key: SegmentId (string). ISA-95 Process Segment. A discrete manufacturing step: Coating, Weaving, SensorEmbed, or Packaging. Maps to L2 process areas Conductive Coating and Weave Line Execution." .

:Material a owl:Class ;
    rdfs:label "Material" ;
    rdfs:comment "Key: MaterialId (string). ISA-95 Material Definition. Raw materials (graphite fiber, silver nanowire, copper trace ink), components (sensor modules, flex PCB), and packaging materials (anti-static bags, ESD shielding)." .

:Supplier a owl:Class ;
    rdfs:label "Supplier" ;
    rdfs:comment "Key: SupplierId (string). External material provider — advanced materials suppliers, sensor component manufacturers, and packaging suppliers." .

:Equipment a owl:Class ;
    rdfs:label "Equipment" ;
    rdfs:comment "Key: EquipmentId (string). ISA-95 Equipment hierarchy (Site, Area, WorkCenter, WorkUnit). Includes innovation centers, weave halls, QA labs, warehouses, supplier sites, and individual machines (FiberWeaver, SensorPlacer, ConductivityTester, Laminator, etc.)." .

:ProductionOrder a owl:Class ;
    rdfs:label "ProductionOrder" ;
    rdfs:comment "Key: OrderId (string). ISA-95 Production Schedule. Work orders for smart mesh production runs, derived from supply plan." .

:QualityTest a owl:Class ;
    rdfs:label "QualityTest" ;
    rdfs:comment "Key: TestId (string). ISA-95 Quality Test Operations. Covers conductivity tests, tensile strength, mesh gauge checks, contamination tests, visual inspections, and QMS audits." .

:Shipment a owl:Class ;
    rdfs:label "Shipment" ;
    rdfs:comment "Key: ShipmentId (string). Logistics movement — inbound raw materials/components and outbound finished goods." .

# =============================================================================
# ProductBatch Properties
# =============================================================================

:BatchId a owl:DatatypeProperty ;
    rdfs:label "BatchId" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:string .

:Batch_Product a owl:DatatypeProperty ;
    rdfs:label "Batch_Product" ;
    rdfs:comment "Smart mesh product name (e.g. ZavaCore Field Standard, ZavaCore Systems Pro)" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:string .

:Batch_MeshSpec a owl:DatatypeProperty ;
    rdfs:label "Batch_MeshSpec" ;
    rdfs:comment "Mesh specification code from master data (ISA-95 Product Definition)" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:string .

:Batch_Quantity a owl:DatatypeProperty ;
    rdfs:label "Batch_Quantity" ;
    rdfs:comment "Number of smart mesh units in this batch" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:integer .

:Batch_Status a owl:DatatypeProperty ;
    rdfs:label "Batch_Status" ;
    rdfs:comment "Production status (Planned, InProgress, Complete, Shipped)" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:string .

:Batch_CompletionDate a owl:DatatypeProperty ;
    rdfs:label "Batch_CompletionDate" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:dateTime .

:Batch_EquipmentId a owl:DatatypeProperty ;
    rdfs:label "Batch_EquipmentId" ;
    rdfs:comment "FK to Equipment (production site) where batch is manufactured" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:string .

