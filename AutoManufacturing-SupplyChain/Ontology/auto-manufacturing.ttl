@prefix : <http://example.com/automanufacturing#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# =============================================================================
# Ontology Declaration
# =============================================================================

<http://example.com/automanufacturing> a owl:Ontology ;
    rdfs:label "NextGen Motors - Automotive Manufacturing & Supply Chain Ontology" ;
    rdfs:comment "Ontology for automotive manufacturing traceability and supply chain management" .

# =============================================================================
# Entity Classes
# =============================================================================

:Vehicle a owl:Class ;
    rdfs:label "Vehicle" ;
    rdfs:comment "Key: VehicleId (string)" .

:Assembly a owl:Class ;
    rdfs:label "Assembly" ;
    rdfs:comment "Key: AssemblyId (string)" .

:Component a owl:Class ;
    rdfs:label "Component" ;
    rdfs:comment "Key: ComponentId (string)" .

:Supplier a owl:Class ;
    rdfs:label "Supplier" ;
    rdfs:comment "Key: SupplierId (string)" .

:Facility a owl:Class ;
    rdfs:label "Facility" ;
    rdfs:comment "Key: FacilityId (string)" .

:ProductionOrder a owl:Class ;
    rdfs:label "ProductionOrder" ;
    rdfs:comment "Key: OrderId (string)" .

:QualityEvent a owl:Class ;
    rdfs:label "QualityEvent" ;
    rdfs:comment "Key: EventId (string)" .

:Shipment a owl:Class ;
    rdfs:label "Shipment" ;
    rdfs:comment "Key: ShipmentId (string)" .

# =============================================================================
# Vehicle Properties
# =============================================================================

:VehicleId a owl:DatatypeProperty ;
    rdfs:label "VehicleId" ;
    rdfs:domain :Vehicle ;
    rdfs:range xsd:string .

:Vehicle_VIN a owl:DatatypeProperty ;
    rdfs:label "Vehicle_VIN" ;
    rdfs:domain :Vehicle ;
    rdfs:range xsd:string .

:Vehicle_Model a owl:DatatypeProperty ;
    rdfs:label "Vehicle_Model" ;
    rdfs:domain :Vehicle ;
    rdfs:range xsd:string .

:Vehicle_Year a owl:DatatypeProperty ;
    rdfs:label "Vehicle_Year" ;
    rdfs:domain :Vehicle ;
    rdfs:range xsd:integer .

:Vehicle_Status a owl:DatatypeProperty ;
    rdfs:label "Vehicle_Status" ;
    rdfs:domain :Vehicle ;
    rdfs:range xsd:string .

:Vehicle_CompletionDate a owl:DatatypeProperty ;
    rdfs:label "Vehicle_CompletionDate" ;
    rdfs:domain :Vehicle ;
    rdfs:range xsd:dateTime .

:Vehicle_FacilityId a owl:DatatypeProperty ;
    rdfs:label "Vehicle_FacilityId" ;
    rdfs:comment "FK to Facility where vehicle is produced" ;
    rdfs:domain :Vehicle ;
    rdfs:range xsd:string .

