# =============================================================================
# Object Properties (Relationships)
# =============================================================================

:assembledInto a owl:ObjectProperty ;
    rdfs:label "ASSEMBLED_INTO" ;
    rdfs:comment "Assembly is assembled into a Vehicle" ;
    rdfs:domain :Assembly ;
    rdfs:range :Vehicle .

:usesComponent a owl:ObjectProperty ;
    rdfs:label "USES_COMPONENT" ;
    rdfs:comment "Assembly uses Component in manufacturing" ;
    rdfs:domain :Assembly ;
    rdfs:range :Component .

:suppliedBy a owl:ObjectProperty ;
    rdfs:label "SUPPLIED_BY" ;
    rdfs:comment "Component is supplied by Supplier" ;
    rdfs:domain :Component ;
    rdfs:range :Supplier .

:producedAt a owl:ObjectProperty ;
    rdfs:label "PRODUCED_AT" ;
    rdfs:comment "Vehicle is produced at Facility" ;
    rdfs:domain :Vehicle ;
    rdfs:range :Facility .

:orderedFor a owl:ObjectProperty ;
    rdfs:label "ORDERED_FOR" ;
    rdfs:comment "ProductionOrder is ordered for Vehicle" ;
    rdfs:domain :ProductionOrder ;
    rdfs:range :Vehicle .

:affects a owl:ObjectProperty ;
    rdfs:label "AFFECTS" ;
    rdfs:comment "QualityEvent affects Assembly" ;
    rdfs:domain :QualityEvent ;
    rdfs:range :Assembly .

:originatedFrom a owl:ObjectProperty ;
    rdfs:label "ORIGINATED_FROM" ;
    rdfs:comment "Shipment originated from Facility" ;
    rdfs:domain :Shipment ;
    rdfs:range :Facility .

:deliveredTo a owl:ObjectProperty ;
    rdfs:label "DELIVERED_TO" ;
    rdfs:comment "Shipment delivered to Facility" ;
    rdfs:domain :Shipment ;
    rdfs:range :Facility .

:ships_component a owl:ObjectProperty ;
    rdfs:label "SHIPS_COMPONENT" ;
    rdfs:comment "Shipment ships Component" ;
    rdfs:domain :Shipment ;
    rdfs:range :Component .

:operatesFrom a owl:ObjectProperty ;
    rdfs:label "OPERATES_FROM" ;
    rdfs:comment "Supplier operates from Facility" ;
    rdfs:domain :Supplier ;
    rdfs:range :Facility .
