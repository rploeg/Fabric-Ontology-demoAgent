@prefix : <http://example.org/bd-medical-manufacturing#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://example.org/bd-medical-manufacturing> rdf:type owl:Ontology ;
    rdfs:label "BD Medical Manufacturing Ontology" ;
    rdfs:comment "Ontology for Becton Dickinson medical device manufacturing traceability and quality management" .

#################################################################
#    ENTITY TYPES (Classes)
#################################################################

:Product rdf:type owl:Class ;
    rdfs:label "Product" ;
    rdfs:comment "BD medical devices across three business segments" .

:ProductionBatch rdf:type owl:Class ;
    rdfs:label "ProductionBatch" ;
    rdfs:comment "Manufacturing lot with full traceability" .

:Facility rdf:type owl:Class ;
    rdfs:label "Facility" ;
    rdfs:comment "BD manufacturing plants worldwide" .

:Supplier rdf:type owl:Class ;
    rdfs:label "Supplier" ;
    rdfs:comment "Raw material and component vendors" .

:Component rdf:type owl:Class ;
    rdfs:label "Component" ;
    rdfs:comment "Parts and materials used in device assembly" .

:QualityEvent rdf:type owl:Class ;
    rdfs:label "QualityEvent" ;
    rdfs:comment "NCRs, CAPAs, deviations, and inspections" .

:RegulatorySubmission rdf:type owl:Class ;
    rdfs:label "RegulatorySubmission" ;
    rdfs:comment "FDA, CE Mark, and regulatory filings" .

:CustomerComplaint rdf:type owl:Class ;
    rdfs:label "CustomerComplaint" ;
    rdfs:comment "Post-market surveillance and adverse events" .

#################################################################
#    ENTITY KEYS (Datatype Properties)
#################################################################

:ProductId rdf:type owl:DatatypeProperty ;
    rdfs:domain :Product ;
    rdfs:range xsd:string ;
    rdfs:label "ProductId" .

:BatchId rdf:type owl:DatatypeProperty ;
    rdfs:domain :ProductionBatch ;
    rdfs:range xsd:string ;
    rdfs:label "BatchId" .

:FacilityId rdf:type owl:DatatypeProperty ;
    rdfs:domain :Facility ;
    rdfs:range xsd:string ;
    rdfs:label "FacilityId" .

:SupplierId rdf:type owl:DatatypeProperty ;
    rdfs:domain :Supplier ;
    rdfs:range xsd:string ;
    rdfs:label "SupplierId" .

:ComponentId rdf:type owl:DatatypeProperty ;
    rdfs:domain :Component ;
    rdfs:range xsd:string ;
    rdfs:label "ComponentId" .

:EventId rdf:type owl:DatatypeProperty ;
    rdfs:domain :QualityEvent ;
    rdfs:range xsd:string ;
    rdfs:label "EventId" .

:SubmissionId rdf:type owl:DatatypeProperty ;
    rdfs:domain :RegulatorySubmission ;
    rdfs:range xsd:string ;
    rdfs:label "SubmissionId" .

:ComplaintId rdf:type owl:DatatypeProperty ;
    rdfs:domain :CustomerComplaint ;
    rdfs:range xsd:string ;
    rdfs:label "ComplaintId" .

