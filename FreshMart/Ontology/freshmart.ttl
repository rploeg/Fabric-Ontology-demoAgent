@prefix : <http://freshmart.com/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# =============================================================================
# FreshMart Supermarket Ontology
# Domain: Retail / Grocery Supply Chain
# Version: 1.0
# Created: January 2026
# =============================================================================

# -----------------------------------------------------------------------------
# Ontology Declaration
# -----------------------------------------------------------------------------

<http://freshmart.com/ontology> rdf:type owl:Ontology ;
    rdfs:label "FreshMart Supermarket Ontology" ;
    rdfs:comment "Supply chain traceability and food safety ontology for FreshMart retail operations" .

# =============================================================================
# ENTITY CLASS DEFINITIONS
# =============================================================================

# -----------------------------------------------------------------------------
# Store Entity
# -----------------------------------------------------------------------------

:Store rdf:type owl:Class ;
    rdfs:label "Store" ;
    rdfs:comment "Key: string - Physical supermarket locations in the FreshMart chain" .

:StoreId rdf:type owl:DatatypeProperty ;
    rdfs:label "StoreId" ;
    rdfs:comment "Primary key for Store entity" ;
    rdfs:domain :Store ;
    rdfs:range xsd:string .

:StoreName rdf:type owl:DatatypeProperty ;
    rdfs:label "StoreName" ;
    rdfs:domain :Store ;
    rdfs:range xsd:string .

:Address rdf:type owl:DatatypeProperty ;
    rdfs:label "Address" ;
    rdfs:domain :Store ;
    rdfs:range xsd:string .

:City rdf:type owl:DatatypeProperty ;
    rdfs:label "City" ;
    rdfs:domain :Store ;
    rdfs:range xsd:string .

:State rdf:type owl:DatatypeProperty ;
    rdfs:label "State" ;
    rdfs:domain :Store ;
    rdfs:range xsd:string .

:ZipCode rdf:type owl:DatatypeProperty ;
    rdfs:label "ZipCode" ;
    rdfs:domain :Store ;
    rdfs:range xsd:string .

:OpenDate rdf:type owl:DatatypeProperty ;
    rdfs:label "OpenDate" ;
    rdfs:domain :Store ;
    rdfs:range xsd:dateTime .

:SquareFootage rdf:type owl:DatatypeProperty ;
    rdfs:label "SquareFootage" ;
    rdfs:domain :Store ;
    rdfs:range xsd:integer .

# Store Timeseries Properties (bound via Eventhouse)
:FootTraffic rdf:type owl:DatatypeProperty ;
    rdfs:label "FootTraffic" ;
    rdfs:comment "Timeseries: Hourly customer count" ;
    rdfs:domain :Store ;
    rdfs:range xsd:integer .

:SalesVelocity rdf:type owl:DatatypeProperty ;
    rdfs:label "SalesVelocity" ;
    rdfs:comment "Timeseries: Units sold per hour" ;
    rdfs:domain :Store ;
    rdfs:range xsd:double .

:AvgTransactionValue rdf:type owl:DatatypeProperty ;
    rdfs:label "AvgTransactionValue" ;
    rdfs:comment "Timeseries: Average basket value in dollars" ;
    rdfs:domain :Store ;
    rdfs:range xsd:double .

# -----------------------------------------------------------------------------
# Product Entity
# -----------------------------------------------------------------------------

:Product rdf:type owl:Class ;
    rdfs:label "Product" ;
    rdfs:comment "Key: string - SKUs sold across FreshMart stores" .

:ProductId rdf:type owl:DatatypeProperty ;
    rdfs:label "ProductId" ;
    rdfs:comment "Primary key for Product entity" ;
    rdfs:domain :Product ;
    rdfs:range xsd:string .

:ProductName rdf:type owl:DatatypeProperty ;
    rdfs:label "ProductName" ;
    rdfs:domain :Product ;
    rdfs:range xsd:string .

:UPC rdf:type owl:DatatypeProperty ;
    rdfs:label "UPC" ;
    rdfs:comment "Universal Product Code barcode" ;
    rdfs:domain :Product ;
    rdfs:range xsd:string .

:UnitPrice rdf:type owl:DatatypeProperty ;
    rdfs:label "UnitPrice" ;
    rdfs:comment "Retail price per unit" ;
    rdfs:domain :Product ;
    rdfs:range xsd:double .

:UnitCost rdf:type owl:DatatypeProperty ;
    rdfs:label "UnitCost" ;
    rdfs:comment "Wholesale cost per unit" ;
    rdfs:domain :Product ;
    rdfs:range xsd:double .

:ShelfLife rdf:type owl:DatatypeProperty ;
    rdfs:label "ShelfLife" ;
    rdfs:comment "Expected shelf life in days" ;
    rdfs:domain :Product ;
    rdfs:range xsd:integer .

# -----------------------------------------------------------------------------
# Supplier Entity
# -----------------------------------------------------------------------------

:Supplier rdf:type owl:Class ;
    rdfs:label "Supplier" ;
    rdfs:comment "Key: string - Vendors providing products to FreshMart" .

:SupplierId rdf:type owl:DatatypeProperty ;
    rdfs:label "SupplierId" ;
    rdfs:comment "Primary key for Supplier entity" ;
    rdfs:domain :Supplier ;
    rdfs:range xsd:string .

:SupplierName rdf:type owl:DatatypeProperty ;
    rdfs:label "SupplierName" ;
    rdfs:domain :Supplier ;
    rdfs:range xsd:string .

:ContactEmail rdf:type owl:DatatypeProperty ;
    rdfs:label "ContactEmail" ;
    rdfs:domain :Supplier ;
    rdfs:range xsd:string .

:Phone rdf:type owl:DatatypeProperty ;
    rdfs:label "Phone" ;
    rdfs:domain :Supplier ;
    rdfs:range xsd:string .

:Country rdf:type owl:DatatypeProperty ;
    rdfs:label "Country" ;
    rdfs:domain :Supplier ;
    rdfs:range xsd:string .

:Rating rdf:type owl:DatatypeProperty ;
    rdfs:label "Rating" ;
    rdfs:comment "Supplier quality rating 0-5" ;
    rdfs:domain :Supplier ;
    rdfs:range xsd:double .

# -----------------------------------------------------------------------------
# ProductBatch Entity
# -----------------------------------------------------------------------------

:ProductBatch rdf:type owl:Class ;
    rdfs:label "ProductBatch" ;
    rdfs:comment "Key: string - Specific shipment/lot of products with tracking" .

:BatchId rdf:type owl:DatatypeProperty ;
    rdfs:label "BatchId" ;
    rdfs:comment "Primary key for ProductBatch entity" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:string .

:LotNumber rdf:type owl:DatatypeProperty ;
    rdfs:label "LotNumber" ;
    rdfs:comment "Manufacturer lot/batch number" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:string .

:ManufactureDate rdf:type owl:DatatypeProperty ;
    rdfs:label "ManufactureDate" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:dateTime .

:ExpiryDate rdf:type owl:DatatypeProperty ;
    rdfs:label "ExpiryDate" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:dateTime .

:Quantity rdf:type owl:DatatypeProperty ;
    rdfs:label "Quantity" ;
    rdfs:comment "Number of units in batch" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:integer .

:BatchStatus rdf:type owl:DatatypeProperty ;
    rdfs:label "BatchStatus" ;
    rdfs:comment "Current status: Active, Expired, Recalled, Sold" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:string .

# ProductBatch Timeseries Properties (bound via Eventhouse)
:StorageTemperature rdf:type owl:DatatypeProperty ;
    rdfs:label "StorageTemperature" ;
    rdfs:comment "Timeseries: Temperature in Celsius" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:double .

:Humidity rdf:type owl:DatatypeProperty ;
    rdfs:label "Humidity" ;
    rdfs:comment "Timeseries: Relative humidity percentage" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:double .

:DaysToExpiry rdf:type owl:DatatypeProperty ;
    rdfs:label "DaysToExpiry" ;
    rdfs:comment "Timeseries: Days remaining until expiry" ;
    rdfs:domain :ProductBatch ;
    rdfs:range xsd:integer .

# -----------------------------------------------------------------------------
# Category Entity
# -----------------------------------------------------------------------------

:Category rdf:type owl:Class ;
    rdfs:label "Category" ;
    rdfs:comment "Key: string - Product categories and departments" .

:CategoryId rdf:type owl:DatatypeProperty ;
    rdfs:label "CategoryId" ;
    rdfs:comment "Primary key for Category entity" ;
    rdfs:domain :Category ;
    rdfs:range xsd:string .

:CategoryName rdf:type owl:DatatypeProperty ;
    rdfs:label "CategoryName" ;
    rdfs:domain :Category ;
    rdfs:range xsd:string .

:Department rdf:type owl:DatatypeProperty ;
    rdfs:label "Department" ;
    rdfs:comment "Store department: Grocery, Produce, Dairy, Frozen, Bakery, Meat" ;
    rdfs:domain :Category ;
    rdfs:range xsd:string .

:IsPerishable rdf:type owl:DatatypeProperty ;
    rdfs:label "IsPerishable" ;
    rdfs:comment "Whether products in category are perishable" ;
    rdfs:domain :Category ;
    rdfs:range xsd:boolean .

# -----------------------------------------------------------------------------
# Employee Entity
# -----------------------------------------------------------------------------

:Employee rdf:type owl:Class ;
    rdfs:label "Employee" ;
    rdfs:comment "Key: string - Store staff members" .

:EmployeeId rdf:type owl:DatatypeProperty ;
    rdfs:label "EmployeeId" ;
    rdfs:comment "Primary key for Employee entity" ;
    rdfs:domain :Employee ;
    rdfs:range xsd:string .

:FirstName rdf:type owl:DatatypeProperty ;
    rdfs:label "FirstName" ;
    rdfs:domain :Employee ;
    rdfs:range xsd:string .

:LastName rdf:type owl:DatatypeProperty ;
    rdfs:label "LastName" ;
    rdfs:domain :Employee ;
    rdfs:range xsd:string .

:Role rdf:type owl:DatatypeProperty ;
    rdfs:label "Role" ;
    rdfs:comment "Job role: Manager, Supervisor, Clerk, QA Inspector" ;
    rdfs:domain :Employee ;
    rdfs:range xsd:string .

:HireDate rdf:type owl:DatatypeProperty ;
    rdfs:label "HireDate" ;
    rdfs:domain :Employee ;
    rdfs:range xsd:dateTime .

:IsActive rdf:type owl:DatatypeProperty ;
    rdfs:label "IsActive" ;
    rdfs:comment "Whether employee is currently active" ;
    rdfs:domain :Employee ;
    rdfs:range xsd:boolean .

# -----------------------------------------------------------------------------
# PurchaseOrder Entity
# -----------------------------------------------------------------------------

:PurchaseOrder rdf:type owl:Class ;
    rdfs:label "PurchaseOrder" ;
    rdfs:comment "Key: string - Orders placed to suppliers" .

:OrderId rdf:type owl:DatatypeProperty ;
    rdfs:label "OrderId" ;
    rdfs:comment "Primary key for PurchaseOrder entity" ;
    rdfs:domain :PurchaseOrder ;
    rdfs:range xsd:string .

:OrderDate rdf:type owl:DatatypeProperty ;
    rdfs:label "OrderDate" ;
    rdfs:domain :PurchaseOrder ;
    rdfs:range xsd:dateTime .

:ExpectedDelivery rdf:type owl:DatatypeProperty ;
    rdfs:label "ExpectedDelivery" ;
    rdfs:domain :PurchaseOrder ;
    rdfs:range xsd:dateTime .

:TotalAmount rdf:type owl:DatatypeProperty ;
    rdfs:label "TotalAmount" ;
    rdfs:comment "Total order value in dollars" ;
    rdfs:domain :PurchaseOrder ;
    rdfs:range xsd:double .

:OrderStatus rdf:type owl:DatatypeProperty ;
    rdfs:label "OrderStatus" ;
    rdfs:comment "Order status: Pending, Shipped, Delivered, Cancelled" ;
    rdfs:domain :PurchaseOrder ;
    rdfs:range xsd:string .

# -----------------------------------------------------------------------------
# QualityInspection Entity
# -----------------------------------------------------------------------------

:QualityInspection rdf:type owl:Class ;
    rdfs:label "QualityInspection" ;
    rdfs:comment "Key: string - Food safety and quality checks on batches" .

:InspectionId rdf:type owl:DatatypeProperty ;
    rdfs:label "InspectionId" ;
    rdfs:comment "Primary key for QualityInspection entity" ;
    rdfs:domain :QualityInspection ;
    rdfs:range xsd:string .

:InspectionDate rdf:type owl:DatatypeProperty ;
    rdfs:label "InspectionDate" ;
    rdfs:domain :QualityInspection ;
    rdfs:range xsd:dateTime .

:Result rdf:type owl:DatatypeProperty ;
    rdfs:label "Result" ;
    rdfs:comment "Inspection result: Pass, Fail, Conditional" ;
    rdfs:domain :QualityInspection ;
    rdfs:range xsd:string .

:Notes rdf:type owl:DatatypeProperty ;
    rdfs:label "Notes" ;
    rdfs:comment "Inspector notes and observations" ;
    rdfs:domain :QualityInspection ;
    rdfs:range xsd:string .

:Score rdf:type owl:DatatypeProperty ;
    rdfs:label "Score" ;
    rdfs:comment "Quality score 0-100" ;
    rdfs:domain :QualityInspection ;
    rdfs:range xsd:integer .

# =============================================================================
# RELATIONSHIP DEFINITIONS (Object Properties)
# =============================================================================

# -----------------------------------------------------------------------------
# BELONGS_TO: Product → Category
# -----------------------------------------------------------------------------

:BELONGS_TO rdf:type owl:ObjectProperty ;
    rdfs:label "BELONGS_TO" ;
    rdfs:comment "Product classification into category" ;
    rdfs:domain :Product ;
    rdfs:range :Category .

# -----------------------------------------------------------------------------
# SUPPLIED_BY: Product → Supplier
# -----------------------------------------------------------------------------

:SUPPLIED_BY rdf:type owl:ObjectProperty ;
    rdfs:label "SUPPLIED_BY" ;
    rdfs:comment "Which supplier provides this product" ;
    rdfs:domain :Product ;
    rdfs:range :Supplier .

# -----------------------------------------------------------------------------
# STOCKS: Store → Product
# -----------------------------------------------------------------------------

:STOCKS rdf:type owl:ObjectProperty ;
    rdfs:label "STOCKS" ;
    rdfs:comment "Products available at each store location" ;
    rdfs:domain :Store ;
    rdfs:range :Product .

# -----------------------------------------------------------------------------
# EMPLOYS: Store → Employee
# -----------------------------------------------------------------------------

:EMPLOYS rdf:type owl:ObjectProperty ;
    rdfs:label "EMPLOYS" ;
    rdfs:comment "Staff assignments to store locations" ;
    rdfs:domain :Store ;
    rdfs:range :Employee .

# -----------------------------------------------------------------------------
# CONTAINS: ProductBatch → Product
# -----------------------------------------------------------------------------

:CONTAINS rdf:type owl:ObjectProperty ;
    rdfs:label "CONTAINS" ;
    rdfs:comment "Which product is contained in this batch" ;
    rdfs:domain :ProductBatch ;
    rdfs:range :Product .

# -----------------------------------------------------------------------------
# RECEIVED_AT: ProductBatch → Store
# -----------------------------------------------------------------------------

:RECEIVED_AT rdf:type owl:ObjectProperty ;
    rdfs:label "RECEIVED_AT" ;
    rdfs:comment "Store that received this product batch" ;
    rdfs:domain :ProductBatch ;
    rdfs:range :Store .

# -----------------------------------------------------------------------------
# ORDERED_BY: PurchaseOrder → Store
# -----------------------------------------------------------------------------

:ORDERED_BY rdf:type owl:ObjectProperty ;
    rdfs:label "ORDERED_BY" ;
    rdfs:comment "Which store placed this purchase order" ;
    rdfs:domain :PurchaseOrder ;
    rdfs:range :Store .

# -----------------------------------------------------------------------------
# FULFILLED_BY: PurchaseOrder → Supplier
# -----------------------------------------------------------------------------

:FULFILLED_BY rdf:type owl:ObjectProperty ;
    rdfs:label "FULFILLED_BY" ;
    rdfs:comment "Supplier fulfilling this purchase order" ;
    rdfs:domain :PurchaseOrder ;
    rdfs:range :Supplier .

# -----------------------------------------------------------------------------
# INSPECTED: QualityInspection → ProductBatch
# -----------------------------------------------------------------------------

:INSPECTED rdf:type owl:ObjectProperty ;
    rdfs:label "INSPECTED" ;
    rdfs:comment "Quality check performed on this batch" ;
    rdfs:domain :QualityInspection ;
    rdfs:range :ProductBatch .

# -----------------------------------------------------------------------------
# PERFORMED_BY: QualityInspection → Employee
# -----------------------------------------------------------------------------

:PERFORMED_BY rdf:type owl:ObjectProperty ;
    rdfs:label "PERFORMED_BY" ;
    rdfs:comment "Employee who conducted this inspection" ;
    rdfs:domain :QualityInspection ;
    rdfs:range :Employee .

# =============================================================================
# END OF ONTOLOGY
# =============================================================================
