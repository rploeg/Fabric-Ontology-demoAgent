# =============================================================================
# OBJECT PROPERTIES (Relationships)
# =============================================================================

# -----------------------------------------------------------------------------
# Customer Relationships
# -----------------------------------------------------------------------------

fg:OWNS rdf:type owl:ObjectProperty ;
    rdfs:label "OWNS" ;
    rdfs:comment "Customer owns CreditCard" ;
    rdfs:domain fg:Customer ;
    rdfs:range fg:CreditCard .

fg:REGISTERED_DEVICE rdf:type owl:ObjectProperty ;
    rdfs:label "REGISTERED_DEVICE" ;
    rdfs:comment "Customer has registered Device" ;
    rdfs:domain fg:Customer ;
    rdfs:range fg:Device .

fg:HAS_ALERT rdf:type owl:ObjectProperty ;
    rdfs:label "HAS_ALERT" ;
    rdfs:comment "Customer has FraudAlert" ;
    rdfs:domain fg:Customer ;
    rdfs:range fg:FraudAlert .

# -----------------------------------------------------------------------------
# CreditCard Relationships
# -----------------------------------------------------------------------------

fg:HAS_TRANSACTION rdf:type owl:ObjectProperty ;
    rdfs:label "HAS_TRANSACTION" ;
    rdfs:comment "CreditCard has Transaction" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range fg:Transaction .

# -----------------------------------------------------------------------------
# Transaction Relationships
# -----------------------------------------------------------------------------

fg:OCCURRED_AT rdf:type owl:ObjectProperty ;
    rdfs:label "OCCURRED_AT" ;
    rdfs:comment "Transaction occurred at Merchant" ;
    rdfs:domain fg:Transaction ;
    rdfs:range fg:Merchant .

fg:TRIGGERED rdf:type owl:ObjectProperty ;
    rdfs:label "TRIGGERED" ;
