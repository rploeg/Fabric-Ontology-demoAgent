@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fg: <http://financeguard.com/ontology/creditfraud#> .

# =============================================================================
# Credit Fraud Detection Ontology
# Company: FinanceGuard Bank
# Domain: Credit Card Fraud Detection & Prevention
# Version: 1.0
# Created: January 2026
# =============================================================================

# -----------------------------------------------------------------------------
# Ontology Declaration
# -----------------------------------------------------------------------------

<http://financeguard.com/ontology/creditfraud> rdf:type owl:Ontology ;
    rdfs:label "Credit Fraud Detection Ontology" ;
    rdfs:comment "Ontology for modeling credit card fraud detection relationships" .

# =============================================================================
# CLASS DEFINITIONS (Entities)
# =============================================================================

# -----------------------------------------------------------------------------
# Customer Entity
# -----------------------------------------------------------------------------

fg:Customer rdf:type owl:Class ;
    rdfs:label "Customer" ;
    rdfs:comment "Bank customers with credit accounts" .

fg:CustomerId rdf:type owl:DatatypeProperty ;
    rdfs:label "CustomerId" ;
    rdfs:comment "Key: string" ;
    rdfs:domain fg:Customer ;
    rdfs:range xsd:string .

fg:FullName rdf:type owl:DatatypeProperty ;
    rdfs:label "FullName" ;
    rdfs:domain fg:Customer ;
    rdfs:range xsd:string .

fg:Email rdf:type owl:DatatypeProperty ;
    rdfs:label "Email" ;
    rdfs:domain fg:Customer ;
    rdfs:range xsd:string .

fg:PhoneNumber rdf:type owl:DatatypeProperty ;
    rdfs:label "PhoneNumber" ;
    rdfs:domain fg:Customer ;
    rdfs:range xsd:string .

fg:RiskScore rdf:type owl:DatatypeProperty ;
    rdfs:label "RiskScore" ;
    rdfs:domain fg:Customer ;
    rdfs:range xsd:double .

fg:AccountStatus rdf:type owl:DatatypeProperty ;
    rdfs:label "AccountStatus" ;
    rdfs:domain fg:Customer ;
    rdfs:range xsd:string .

fg:CreatedDate rdf:type owl:DatatypeProperty ;
    rdfs:label "CreatedDate" ;
    rdfs:domain fg:Customer ;
    rdfs:range xsd:dateTime .

# -----------------------------------------------------------------------------
# CreditCard Entity
# -----------------------------------------------------------------------------

fg:CreditCard rdf:type owl:Class ;
    rdfs:label "CreditCard" ;
    rdfs:comment "Credit card accounts linked to customers" .

fg:CardId rdf:type owl:DatatypeProperty ;
    rdfs:label "CardId" ;
    rdfs:comment "Key: string" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range xsd:string .

fg:CardNumber rdf:type owl:DatatypeProperty ;
    rdfs:label "CardNumber" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range xsd:string .

fg:CardType rdf:type owl:DatatypeProperty ;
    rdfs:label "CardType" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range xsd:string .

fg:CreditLimit rdf:type owl:DatatypeProperty ;
    rdfs:label "CreditLimit" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range xsd:double .

fg:IssuedDate rdf:type owl:DatatypeProperty ;
    rdfs:label "IssuedDate" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range xsd:dateTime .

fg:ExpiryDate rdf:type owl:DatatypeProperty ;
    rdfs:label "ExpiryDate" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range xsd:dateTime .

fg:IsActive rdf:type owl:DatatypeProperty ;
    rdfs:label "IsActive" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range xsd:boolean .

# CreditCard Timeseries Properties (bound via Eventhouse)
fg:TxnVelocity rdf:type owl:DatatypeProperty ;
    rdfs:label "TxnVelocity" ;
    rdfs:comment "Timeseries: Transactions per hour" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range xsd:double .

fg:DailySpend rdf:type owl:DatatypeProperty ;
    rdfs:label "DailySpend" ;
    rdfs:comment "Timeseries: Daily spending amount" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range xsd:double .

fg:DeclineRate rdf:type owl:DatatypeProperty ;
    rdfs:label "DeclineRate" ;
    rdfs:comment "Timeseries: Transaction decline rate percentage" ;
    rdfs:domain fg:CreditCard ;
    rdfs:range xsd:double .

# -----------------------------------------------------------------------------
# Transaction Entity
# -----------------------------------------------------------------------------

fg:Transaction rdf:type owl:Class ;
    rdfs:label "Transaction" ;
    rdfs:comment "Individual credit card transactions" .

fg:TransactionId rdf:type owl:DatatypeProperty ;
    rdfs:label "TransactionId" ;
    rdfs:comment "Key: string" ;
    rdfs:domain fg:Transaction ;
    rdfs:range xsd:string .

fg:Amount rdf:type owl:DatatypeProperty ;
    rdfs:label "Amount" ;
    rdfs:domain fg:Transaction ;
    rdfs:range xsd:double .

fg:TransactionType rdf:type owl:DatatypeProperty ;
    rdfs:label "TransactionType" ;
    rdfs:domain fg:Transaction ;
    rdfs:range xsd:string .

fg:TransactionDate rdf:type owl:DatatypeProperty ;
    rdfs:label "TransactionDate" ;
    rdfs:domain fg:Transaction ;
    rdfs:range xsd:dateTime .

fg:IsOnline rdf:type owl:DatatypeProperty ;
    rdfs:label "IsOnline" ;
    rdfs:domain fg:Transaction ;
    rdfs:range xsd:boolean .

fg:RiskFlag rdf:type owl:DatatypeProperty ;
    rdfs:label "RiskFlag" ;
    rdfs:domain fg:Transaction ;
    rdfs:range xsd:string .

