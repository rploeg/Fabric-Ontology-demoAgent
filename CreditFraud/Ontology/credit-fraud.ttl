    rdfs:range fg:Device .

fg:LOCATED_IN_TXN rdf:type owl:ObjectProperty ;
    rdfs:label "LOCATED_IN_TXN" ;
    rdfs:comment "Transaction located in GeoLocation" ;
    rdfs:domain fg:Transaction ;
    rdfs:range fg:GeoLocation .

# -----------------------------------------------------------------------------
# Merchant Relationships
# -----------------------------------------------------------------------------

fg:LOCATED_IN_MERCHANT rdf:type owl:ObjectProperty ;
    rdfs:label "LOCATED_IN_MERCHANT" ;
    rdfs:comment "Merchant located in GeoLocation" ;
    rdfs:domain fg:Merchant ;
    rdfs:range fg:GeoLocation .

# -----------------------------------------------------------------------------
# FraudAlert Relationships
# -----------------------------------------------------------------------------

fg:FLAGGED_CARD rdf:type owl:ObjectProperty ;
    rdfs:label "FLAGGED_CARD" ;
    rdfs:comment "FraudAlert flagged CreditCard" ;
    rdfs:domain fg:FraudAlert ;
    rdfs:range fg:CreditCard .

